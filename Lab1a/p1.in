//P1: Check if 3 lengths given can create a triangle

int a, b, c.
    	cout << "Length of a: ".
    	cin >> a.
    	cout << "Length of b: ".
    	cin >> b.
    	cout << "Length of c: ".
   	cin >> c.

    	if [a + b > c and a + c > b and b + c > a]
